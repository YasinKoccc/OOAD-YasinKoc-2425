<Window x:Class="WpfLandenRaden.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfLandenRaden"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnStart" Content="START" HorizontalAlignment="Left" Margin="350,30,0,0" VerticalAlignment="Top" Width="100" Height="30" FontWeight="Bold" Click="btnStart_Click"/>
        <TextBlock x:Name="txtLand" HorizontalAlignment="Left" Margin="350,80,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="Bold"/>
        <StackPanel x:Name="stackPanel" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,150,0,0" Orientation="Horizontal" Height="246" Width="672"/>
    </Grid>
</Window>
